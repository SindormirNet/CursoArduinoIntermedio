#define POTE A0

const int tabla_calib[204] = {0, 1, 2, 10, 11, 12, 13, 24, 25, 26, 30, 31, 45, 47, 48, 50, 51, 59, 71, 81, 91, 99,
                              100, 101, 102, 110, 111, 112, 113, 124, 125, 126, 130, 131, 131, 145, 147, 148, 150, 151, 159, 171, 181, 191, 199,
                              200, 201, 202, 210, 211, 212, 213, 224, 225, 226, 230, 231, 231, 245, 247, 248, 250, 251, 259, 271, 281, 291, 299,
                              300, 301, 302, 310, 311, 312, 313, 324, 325, 326, 330, 331, 331, 345, 347, 348, 350, 351, 359, 371, 381, 391, 399,
                              400, 401, 402, 410, 411, 412, 413, 424, 425, 426, 430, 431, 431, 445, 447, 448, 450, 451, 459, 471, 481, 491, 499,
                              500, 501, 502, 510, 511, 512, 513, 524, 525, 526, 530, 531, 531, 545, 547, 548, 550, 551, 559, 571, 581, 591, 599,
                              600, 601, 602, 610, 611, 612, 613, 624, 625, 626, 630, 631, 631, 645, 647, 648, 650, 651, 659, 671, 681, 691, 699,
                              700, 701, 702, 710, 711, 712, 713, 724, 725, 726, 730, 731, 731, 745, 747, 748, 750, 751, 759, 771, 781, 791, 799,
                              800, 801, 802, 810, 811, 812, 813, 824, 825, 826, 830, 831, 831, 845, 847, 848, 850, 851, 859, 871, 881
                             };
void setup() {
  Serial.begin(9600);
  pinMode(POTE, INPUT);

}

void loop() {
  int lectura_bruto;
  int valor_calibrado;
  
  lectura_bruto = analogRead(POTE);

  valor_calibrado = tabla_calib[lectura_bruto];

  //Serial.print("Lectura pote es: ");
  Serial.print(lectura_bruto);
  Serial.print(" ");
  Serial.println(valor_calibrado);
  delay(10);

}
